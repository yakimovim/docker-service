@model Service.Controllers.EnvironmentType[];
@{
    Layout = "_Layout";
}

@foreach (var environmentType in Model)
{
    <h2>@environmentType.Name</h2>

    <table class="table table-striped table-bordered">
        <tbody>
            @foreach (var environmentVariable in environmentType.Variables)
            {
                <tr>
                    <td>@environmentVariable.Name</td>
                    <td>@environmentVariable.Value</td>
                </tr>
            }

        </tbody>
    </table>
}
